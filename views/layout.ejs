    <script src="/linker/bower_components/jquery/jquery-migrate.min.js"></script>
    <script src="/linker/bower_components/jquery/jquery.js"></script>
    <script src="/linker/bower_components/topcoat/Gruntfile.js"></script>
    <script src="/linker/bower_components/topcoat/demo/js/main.js"></script>
    <script src="/linker/bower_components/topcoat/demo/js/rainbow-custom.min.js"></script>
    <script src="/linker/bower_components/topcoat/demo/js/rainbow.linenumbers.min.js"></script>
    <script src="/linker/bower_components/topcoat/dev/build/styleguide.js"></script>
